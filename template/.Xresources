Rxvt.background: black
Rxvt.foreground: white
Rxvt.font: xft:RobotoMono-Medium:size=10:antialias=true
Rxvt.boldFont: xft:RobotoMono-Bold:size=10:antialias=true
Rxvt.italicFont: xft:RobotoMono-MediumItalic:size=10:antialias=true
Rxvt.boldItalicFont: xft:RobotoMono-BoldItalic:size=10:antialias=true
Rxvt.scrollBar: false
Rxvt.cursorBlink: true
Rxvt.urlLauncher: google-chrome
Rxvt.saveLines: 10000
Rxvt.scrollTtyOutput: false
Rxvt.scrollWithBuffer: false
Rxvt.scrollTtyKeypress: true
Rxvt.secondaryScreen: 1
Rxvt.secondaryScroll: 0
Rxvt.secondaryWheel: 1

